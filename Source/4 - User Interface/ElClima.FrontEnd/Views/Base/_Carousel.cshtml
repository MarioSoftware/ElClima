<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src="~/assets/images/1200px-Rueda_Eiffel_3.jpg" alt="First slide">
            <div class="carousel-caption d-none d-md-block">
                <h5>Hermosa mi córdoba</h5>
                <p>22:30 pm</p>
            </div>
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/assets/images/plaza.jpg" alt="Second slide">
            <div class="carousel-caption d-none d-md-block">
                <h5>La vista desde mi ventana esta mañana</h5>
                <p>11:02 pm</p>
            </div>
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/assets/images/cordoba tarde noche.jpg" alt="Third slide">
            <div class="carousel-caption d-none d-md-block">
                <h5>Impactante la vista</h5>
                <p>14:43 pm</p>
            </div>
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/assets/images/maxresdefault.jpg" alt="Third slide">
            <div class="carousel-caption d-none d-md-block">
                <h5>Bello!</h5>
                <p>14:43 pm</p>
            </div>
        </div>
    </div>
