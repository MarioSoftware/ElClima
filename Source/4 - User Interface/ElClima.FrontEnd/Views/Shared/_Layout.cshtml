@using Microsoft.AspNetCore.Mvc.Rendering
@using ElClima.FrontEnd.Helpers  

<!doctype html>
<html lang="es-AR">
<head>
    <link rel="shortcut icon" type="image/ico" href="~/favicon.ico">
    <meta charset="UTF-8/ISO-8859-1" />
    <meta http-equiv="Content-type" content="text/html; charset=utf-8" />

    <meta name="description" content="" />
    <meta name="Author" content="MarioPedernera" />

 
 

    @RenderSection("HtmlHead", false)

</head>
<body>
    <!-- WRAPPER -->

    <div id="wrapper">

        @*@await Html.PartialAsync("_HeaderAndPanelPartial")*@

        <!--     MIDDLE     -->
        <section id="middle">
            @RenderBody()
        </section>
        <!-- /MIDDLE -->

    </div>
 
    
    @RenderSection("DelayedScripts", false)
    @Html.Raw(MasterLayoutHelper.GetDelayedJavaScriptFiles(Context))
    <script type="text/javascript">
        jQuery(document).ready(function () {
            @Html.Raw(MasterLayoutHelper.GetLinesForDocumentReadyFunction(Context))
        });
    </script>
</body>
</html>
